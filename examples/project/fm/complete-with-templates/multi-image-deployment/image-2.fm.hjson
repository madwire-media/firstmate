{
    kind: step/empty
    description: profile combiner for image-2

    profiles: {
        *: {
            steps: {
                image: ./default
            }

            outputs: {
                url: ${steps.image.url}
            }
        }

        local: {
            steps: {
                image: ./local
            }

            outputs: {
                url: ${steps.image.url}
                proxyId: ${steps.local.proxyId}
            }
        }
    }
}
