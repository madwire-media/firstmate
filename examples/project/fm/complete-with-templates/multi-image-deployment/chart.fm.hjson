{
    kind: template/built-in/helm
    description: example templated chart and release

    profiles: {
        *: {
            requiredParams: [
                image-1
                image-2
            ]

            values: {
                images: {
                    image1: ${params.image-1}
                    image2: ${params.image-2}
                }

                proxyId: ${params.proxyId}
            }
        }
    }
}
