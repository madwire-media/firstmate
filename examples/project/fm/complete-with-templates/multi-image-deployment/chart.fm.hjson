{
    kind: template/std/helm/basic-release
    description: example templated chart and release

    profiles: {
        *: {
            requiredParams: [
                image-1-url
                image-2-url
            ]

            values: {
                images: {
                    image1: ${params.image-1-url}
                    image2: ${params.image-2-url}
                }

                image2ProxyId: ${params.image-2-proxyId}
            }
        }
    }
}
