{
    kind: step/empty
    description: profile combiner for image-2

    profiles: {
        *: {
            steps: {
                push: ./default/push
            }
            outputs: {
                image: ${steps.push.url}
            }
        }

        local: {
            steps: {
                local: ./local/run
            }
            outputs: {
                image: ${steps.local.proxyImage}
                proxyId: ${steps.local.proxyId}
            }
        }
    }
}
