{
    project: test
    services: {
        test: {
            type: dockerImage
            branches: {
                ~default: {
                    dev: {
                        registry: registry.com
                        imageName: image
                        dockerArgs: {
                            foo: bar
                            mahna: mahna
                        }

                        pushImage: true
                    }
                    stage: {
                        registry: registry.com
                        imageName: image
                        dockerArgs: {
                            foo: bar
                            mahna: mahna
                        }

                        pushImage: true
                    }
                    prod: {
                        registry: registry.com
                        imageName: image
                        dockerArgs: {
                            foo: bar
                            mahna: mahna
                        }

                        version: test
                    }
                }
                dev: {
                    inheritFrom: ~default
                    allowedModes: ['dev']
                }
                stage: {
                    inheritFrom: ~default
                    allowedModes: ['stage']
                }
                prod: {
                    inheritFrom: ~default
                    allowedModes: ['prod']
                }
            }
        }
    }
}
