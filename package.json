{
    "name": "firstmate",
    "version": "0.1.0",
    "description": "A tool to make development on kubernetes easy",
    "repository": {
        "type": "git",
        "url": "https://github.com/madwire-media/sergeant"
    },
    "author": "Dakota Sullivan",
    "license": "MIT",
    "bin": "dist/main.js",
    "scripts": {
        "start": "node dist/main.js",
        "debug": "npm run build && node --inspect-brk dist/main.js",
