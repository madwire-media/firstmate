{
    "name": "firstmate",
    "version": "0.1.0",
    "description": "A tool to make development on kubernetes easy",
    "repository": {
        "type": "git",
        "url": "https://github.com/madwire-media/firstmate"
    },
    "author": "Dakota Sullivan",
    "license": "MIT",
    "bin": {
        "fm": "dist/main.js"
    },
    "scripts": {
        "start": "node dist/main.js",
        "debug": "npm run build-ts && node --inspect-brk dist/main.js",
        "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage false general/inheritFrom/modes",
        "build": "npm run build-ts && npm run tslint",
        "build-ts": "tsc",
        "watch": "tsc --watch",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "test": "jest"
    },
    "dependencies": {
        "ajv": "^6.7.0",
        "dockerfile-ast": "0.0.12",
        "fp-ts": "^1.13.0",
        "hjson": "git+https://github.com/dqsully/hjson-js.git",
        "io-ts": "^1.7.0",
        "leven": "^2.1.0",
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0",
        "parse-git-config": "^2.0.3",
        "request": "^2.88.0",
        "rimraf": "^2.6.3",
        "semver": "^5.6.0",
        "short-ansi": "^1.0.2",
        "sywac": "^1.2.1",
        "ta-json": "^2.5.0"
    },
    "devDependencies": {
        "@types/jest": "^23.3.13",
        "@types/leven": "^2.1.1",
        "@types/mkdirp": "^0.5.2",
        "@types/ncp": "^2.0.1",
        "@types/node": "^10.12.18",
        "@types/parse-git-config": "^2.0.2",
        "@types/request": "^2.48.1",
        "@types/rimraf": "^2.0.2",
        "@types/semver": "^5.5.0",
        "concurrently": "^3.6.1",
        "jest": "^23.6.0",
        "jest-cli": "^23.6.0",
        "ts-jest": "^23.10.5",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.1",
        "typescript": "^3.2.4"
    }
}
