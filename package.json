        "short-ansi": "^1.0.1",
        "sywac": "^1.2.1"
    },
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "@types/leven": "^2.1.1",
        "@types/mkdirp": "^0.5.2",
        "@types/ncp": "^2.0.1",
        "@types/node": "^10.5.1",
        "@types/parse-git-config": "^2.0.0",
        "@types/request": "^2.47.1",
        "@types/rimraf": "^2.0.2",
        "concurrently": "^3.6.0",
        "jest": "^23.5.0",
